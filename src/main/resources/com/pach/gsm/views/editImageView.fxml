<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="812.0" prefWidth="880.0" styleClass="backgroundPane" stylesheets="@../../../../styles/dark.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pach.gsm.controllers.editImageViewController">
   <children>
      <VBox prefHeight="812.0" prefWidth="880.0">
         <children>
            <HBox prefHeight="813.0" prefWidth="880.0">
               <children>
                  <VBox id="leftMenu" alignment="TOP_CENTER" prefHeight="813.0" prefWidth="234.0" spacing="10.0" styleClass="auxiliaryMenu">
                     <children>
                        <ImageView fx:id="image1" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
                        <ImageView fx:id="image2" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
                        <ImageView fx:id="image3" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </VBox>
                  <VBox prefHeight="641.0" prefWidth="770.0">
                     <children>
                        <HBox alignment="CENTER" prefHeight="67.0" prefWidth="620.0" styleClass="paneTitle">
                           <children>
                              <Label text="Edit Image" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="700.0" prefWidth="688.0">
                           <children>
                              <HBox fx:id="twoImageLayoutOne" alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="680.0" prefWidth="689.0" spacing="10.0" styleClass="layoutPaneHolder">
                                 <children>
                                    <StackPane fx:id="twoImageLayoutOneSlot1" prefHeight="580.0" prefWidth="300.0" styleClass="layoutPane" HBox.hgrow="ALWAYS" />
                                    <StackPane fx:id="twoImageLayoutOneSlot2" prefHeight="580.0" prefWidth="300.0" styleClass="layoutPane" HBox.hgrow="ALWAYS" />
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </HBox>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox id="bottomMenu" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" styleClass="auxiliaryMenu">
               <children>
                  <Button fx:id="cancelImageEdit" mnemonicParsing="false" text="Cancel" />
                  <Button fx:id="acceptImageEdit" mnemonicParsing="false" text="Use this image" />
               </children>
               <padding>
                  <Insets left="30.0" right="30.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
